@model StudentAchievement.Service.Models.CourseDto

@{
    ViewData["Title"] = "SaveData";
}

<form asp-action="CreateNewReportAsync">
    <div  style="color:red"  asp-validation-summary="All"></div>
    <div class="row">
        <input asp-for="@Model.Id" type="hidden" />
        <div class="form-row">
            
            <div class="form-group">
                <label asp-for="Name" class="control-label">Название предмета</label>
                <input id="countOfKg" asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>

            </div>
        </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-primary" />
    </div>
</form>



